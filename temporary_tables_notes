FROM instruments;

CREATE TABLE stuff(
	name VARCHAR(200),
	quantity INT UNSIGNED
	);
	
INSERT INTO stuff (name, quantity)
VALUES 
("do-hicky", 4),
("yo-yo", 9),
("shoe horn", 420);

SELECT *
FROM stuff;

DROP TABLE stuff;